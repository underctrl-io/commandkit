{
  "name": "commandkit",
  "description": "Beginner friendly command & event handler for Discord.js",
  "version": "1.0.0",
  "license": "MIT",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./bin/index.mjs",
  "files": [
    "dist",
    "bin",
    "./jsx-runtime.cjs",
    "./jsx-runtime.mjs",
    "./jsx-runtime.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./jsx-runtime": {
      "require": "./jsx-runtime.cjs",
      "import": "./jsx-runtime.mjs",
      "types": "./jsx-runtime.d.ts"
    }
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "dev": "pnpm run --filter=test-bot dev",
    "build": "tsup",
    "publish-package": "npm publish",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/underctrl-io/commandkit",
    "directory": "packages/commandkit"
  },
  "homepage": "https://commandkit.dev",
  "keywords": [
    "discord.js",
    "command handler",
    "event handler"
  ],
  "dependencies": {
    "chokidar": "^4.0.3",
    "commander": "^12.1.0",
    "directive-to-hof": "^0.0.2",
    "dotenv": "^16.4.7",
    "ms": "^2.1.3",
    "ora": "^8.0.1",
    "picocolors": "^1.1.1",
    "rfdc": "^1.3.1",
    "rimraf": "^6.0.0",
    "tsup": "^8.4.0",
    "use-macro": "^1.1.0"
  },
  "devDependencies": {
    "@types/ms": "^0.7.34",
    "@types/node": "^22.10.2",
    "@types/yargs": "^17.0.32",
    "discord.js": "^14.19.1",
    "tsconfig": "workspace:*",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "discord.js": "^14"
  },
  "engines": {
    "node": ">=22"
  }
}